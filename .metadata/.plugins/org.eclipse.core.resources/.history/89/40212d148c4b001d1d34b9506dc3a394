package com.assessment.one;

import java.util.HashMap;
import java.util.LinkedHashMap;

public class MainClass {

	public static void main(String[] args) {
		Employee e1=new Employee(1, "Swasti", 20000);
		Employee e2=new Employee(1, "Swasti",20000);
		Employee e3=new Employee(3, "Srishti", 2000);
		Employee e4=new Employee(4, "Shreshth", 5000);
		Employee e5=new Employee(5, "Pragya", 30000);
		
        HashMap<Employee, String> employeeMap = new LinkedHashMap<>();
        employeeMap.put(e1, "Telus");
        employeeMap.put(e2, "Google");
        employeeMap.put(e3, "Microsoft");        
        employeeMap.put(e4, "Infosys");
        employeeMap.put(e5, "Amazon");
        
        System.out.println(employeeMap);


        
        

	}
	

}
